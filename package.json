{
	"name": "@anephenix/mcg",
	"version": "0.0.3",
	"description": "Model Component Generator for Objection.js",
	"main": "index.js",
	"bin": "./bin/mcg",
	"scripts": {
		"lint": "npx eslint .",
		"test": "NODE_ENV=test jest --runInBand --coverage",
		"coveralls": "NODE_ENV=test jest --runInBand --coverage --coverageReporters=text-lcov | coveralls"
	},
	"keywords": [
		"objection",
		"model",
		"generator"
	],
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm t"
		}
	},
	"author": "Paul Jensen <paul@anephenix.com>",
	"license": "MIT",
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"coveralls": "^3.1.0",
		"eslint": "^7.7.0",
		"eslint-plugin-jest": "^23.20.0",
		"husky": "^4.2.5",
		"jest": "^26.4.2",
		"npm-upgrade": "^2.0.4",
		"prettier": "^2.1.1"
	},
	"dependencies": {
		"commander": "^6.1.0",
		"pluralize": "^8.0.0",
		"to-case": "^2.0.0"
	}
}
