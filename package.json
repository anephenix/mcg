{
	"name": "@anephenix/mcg",
	"version": "0.0.3",
	"description": "Model Component Generator for Objection.js",
	"main": "index.js",
	"bin": "./bin/mcg",
	"scripts": {
		"cover": "cross-env NODE_ENV=test npx nyc --reporter=lcov npm t",
		"lint": "npx eslint .",
		"test": "cross-env NODE_ENV=test npx mocha --require ts-node/register --extensions ts,tsx __tests__/* --exclude=__tests__/data/* --test"
	},
	"keywords": [
		"objection",
		"model",
		"generator"
	],
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm t"
		}
	},
	"author": "Paul Jensen <paul@anephenix.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"babel-eslint": "^10.1.0",
		"coveralls": "^3.1.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.0.1",
		"eslint-plugin-jest": "^25.2.2",
		"husky": "^9.1.7",
		"mocha": "^11.0.1",
		"nyc": "^17.1.0",
		"prettier": "^3.4.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"commander": "^8.2.0",
		"pluralize": "^8.0.0",
		"to-case": "^2.0.0"
	}
}
